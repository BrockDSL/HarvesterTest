conversation_id": "1589677576896671745", "entities": {"urls": [{"start": 234, "end": 257, "url": "https://t.co/FHAKW0DrWG", "expanded_url": "https://library.princeton.edu/news/general/2022-10-31/jen-grayburn-assistant-director-digital-open-scholarship-qa", "display_url": "library.princeton.edu/news/general/2\u2026", "images": [{"url": "https://pbs.twimg.com/news_img/1589677582567428096/z7dwoi8W?format=jpg&name=orig", "width": 500, "height": 500}, {"url": "https://pbs.twimg.com/news_img/1589677582567428096/z7dwoi8W?format=jpg&name=150x150", "width": 150, "height": 150}], "status": 200, "title": "Jen Grayburn, Assistant Director of Digital & Open Scholarship: Q&A | Princeton University Library", "description": "Before joining Princeton University Library as the Assistant Director of Digital & Open Scholarship, Dr. Jennifer Grayburn worked as the Director of Digital Scholarship & Public Services at Union College. Jennifer also worked as a Council on Library & Information Resources (CLIR) postdoctoral fellow at Temple University\u2019s Digital Scholarship Center and as a Praxis Fellow and Makerspace Technologist at the University of Virginia\u2019s Scholars\u2019 Lab. Her research interests include immersive and makerspace", "unwound_url": "https://library.princeton.edu/news/general/2022-10-31/jen-grayburn-assistant-director-digital-open-scholarship-qa"}, {"start": 258, "end": 281, "url": "https://t.co/8Dw4Hy0oyy", "expanded_url": "https://twitter.com/PULibrary/status/1589677576896671745/photo/1", "display_url": "pic.twitter.com/8Dw4Hy0oyy", "media_key": "3_1589677572610310146"}], "mentions": [{"start": 29, "end": 41, "username": "JenGrayburn", "id": "3835115957"}]}, "public_metrics": {"retweet_count": 0, "reply_count": 0, "like_count": 2, "quote_count": 0}, "possibly_sensitive": false, "id": "1589677576896671745", "created_at": "2022-11-07T17:54:03.000Z", "source": "Sprout Social", "edit_controls": {"edits_remaining": 5, "is_edit_eligible": true, "editable_until": "2022-11-07T18:24:03.000Z"}, "edit_history_tweet_ids": ["1589677576896671745"], "reply_settings": "everyone", "attachments": {"media_keys": ["3_1589677572610310146"]}, "author_id": "1042365943", "lang": "en", "text": "What is digital scholarship? @JenGrayburn is our new Assistant Director of Digital &amp; Open Scholarship. She and her team support researchers as they navigate this new scholarly ecosystem with a focus on the full project lifecycle. https://t.co/FHAKW0DrWG https://t.co/8Dw4Hy0oyy"}, {"conversation_id": "1589675013569118210", "entities": {"hashtags": [{"start": 0, "end": 10, "tag": "Libraries"}], "urls": [{"start": 251, "end": 274, "url": "https://t.co/8tLBJJkujN", "expanded_url": "https://digitalcommons.fiu.edu/", "display_url": "digitalcommons.fiu.edu", "images": [{"url": "https://pbs.twimg.com/news_img/1589675058334924805/4oCOnoVz?format=jpg&name=orig", "width": 980, "height": 150}, {"url": "https://pbs.twimg.com/news_img/1589675058334924805/4oCOnoVz?format=jpg&name=150x150", "width": 150, "height": 150}], "status": 200, "title": "FIU Digital Commons | Florida International University Research", "description": "Digital Commons is a service of the Florida International University Libraries and provides a centralized access point for scholarship, creative works and history of the FIU community. Faculty, staff and students are encouraged to contribute their work. Sharing your work in Digital Commons provides immediate access to your research and allows you to provide your scholarship openly while still retaining full rights to your work. Contributing authors receive monthly readership reports by email and can", "unwound_url": "https://digitalcommons.fiu.edu/"}, {"start": 275, "end": 298, "url": "https://t.co/SbFjRTtyin", "expanded_url": "https://twitter.com/FIU_Digital_Lib/status/1589675013569118210/video/1", "display_url": "pic.twitter.com/SbFjRTtyin", "media_key": "7_1589674836346945537"}]}, "public_metrics": {"retweet_count": 0, "reply_count": 0, "like_